<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <style>
        h1 {
        margin-top: 0px !important;
        }
        .label {
        font-size: 22.5px;
        fill: #ffffff;
        text-anchor: middle;
        alignment-baseline: middle;
        }
        .face {
        stroke: #c8c8c8;
        stroke-width: 2;
        }
        .minorTicks {
        stroke-width: 2;
        stroke: white;
        }
        .majorTicks {
        stroke: white;
        stroke-width: 3;
        }
        body {
        margin: auto;
        }
    </style>
    <link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
</head>
<body class="container-fluid">
    @RenderBody()
</body>
</html>